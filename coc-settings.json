    "diagnostic.displayByAle": true,
    "inlayHint.enable": false,
    "snippets.priority": 100,
    "suggest.noselect": false,
    "suggest.insertMode": "insert",

    "elixir.pathToElixirLS": "~/Programy/elixir/elixir-ls/release/language_server.sh",

